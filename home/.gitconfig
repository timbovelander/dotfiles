[alias]
  a = add
  b = branch -v
  c = commit
  co = checkout
  d = diff
  f = flow
  ff = flow feature
  ffs = flow feature start
  fff = flow feature finish
  fh = flow hotfix
  fhs = flow hotfix start
  fhf = flow hotfix finish
  fr = flow release
  frs = flow release start
  frf = flow release finish
  fs = flow support
  fss = flow support start
  l = log --pretty=format:'%C(Yellow)%h%C(reset) - %C(cyan)%cr%C(reset) : %C(blue)%an%C(reset)%C(auto)%d%C(reset)%n%s%n' --graph
  lm = "!git log --pretty=format:'%C(Yellow)%h%C(reset) - %C(cyan)%cr%C(reset) : %C(blue)%an%C(reset)%C(auto)%d%C(reset)%n%s%n' --graph --abbrev-commit --author=\"$(git config user.name)\""
  m = merge --no-ff
  pl = pull --rebase=preserve --prune
  ps = push
  r = revert --no-commit
  s = status
  sh = show
  ss = stash -u
  ssl = stash list
  ssa = stash apply
  ssd = stash drop
  ssp = stash pop
[core]
	excludesfile = ~/.gitignore
[push]
  default = simple
[include]
  path = ~/.gitconfig.local
